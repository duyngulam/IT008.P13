<Window x:Class="Line98.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Line98"
        xmlns:views ="clr-namespace:Line98.View"
        xmlns:localViewModel="clr-namespace:Line98.ViewModel"
        mc:Ignorable="d"
        WindowStartupLocation="CenterScreen"
        Title="MainWindow" Height="720" Width="1280">

    <Window.Background >
        <ImageBrush ImageSource="/View/Pics/background.png"

                    Stretch="UniformToFill" />
    </Window.Background>

    <Grid >
        <views:Start />
        <views:MenuView Visibility="Collapsed"/>
    </Grid>
</Window>
